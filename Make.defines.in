# System = i686-linux

CC := gcc
CXX := g++
RANLIB := ranlib
STRIP := strip

CFLAGS += -Wall -I../include
CXXFLAGS += $(CFLAGS)
LIBFLAGS += -ldl -lfg -lbsd -lm -lpthread -lmw -L../lib
DBGFLAGS := -DMEMWATCH -DMW_STDIO

LIBS = libfg.so libdlshared.so

LIBFG_OBJS = config.o substr.o abspath.o strbuf.o usage.o wrapper.o

LIBS_OBJS = $(LIBFG_OBJS)

CLEANFILES = *.o *.so *.a
